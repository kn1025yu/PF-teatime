<h2>新規投稿</h2>
<%= form_with model: @post_image, local:true do |f| %>
<%= f.text_field :title, placeholder: "タイトル" %>
<%= f.text_field :tag_name, placeholder: "タグ" %>
<%= f.text_area :content, size: '30x10', placeholder: "本文" %><br>
<%= f.submit '投稿', class: 'btn btn-success btn-sm btn-block mb-3' %>
<% end %>