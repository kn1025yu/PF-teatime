    <tbody id="post_image">
    <% post_images.each do |post_image| %>
      <tr>
        <td>
          <%= link_to user_path(post_image.user.id) do %>
            <%= attachment_image_tag post_image.user, :profile_image, class: "rounded-circle", format: 'jpeg', fallback: "no_image.png", size: "50xx50" %>
          <% end %>
        </td>
        <td><%= link_to post_image.title, post_image_path(post_image.id) %></td>
        <td></td>
